{
  "env": {
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "space-before-blocks": 2,
    "space-before-keywords": 2,
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": 1
    }],
    "block-scoped-var": 2,
    "eqeqeq": 2,
    "no-multi-spaces": 2,
    "consistent-return": 2,
    "no-extend-native": [2, {
      "exceptions": ["Object"]
    }],
    "curly": 2,
    "no-sequences": 2,
    "no-native-reassign": 2,
    "dot-location": [2, "property"],
    "dot-notation": 1,
    "no-console": 1,
    "semi-spacing": 1,
    "one-var": [2, "never"],
    "newline-after-var": 2
  }
}
